@model List<GroupMember>
@{
    ViewData["Title"] = "Dashboard";
}
<h1>Dashboard</h1>
<div class="well-lg">
    <div class="container-fluid">
        <div class="row">

            @foreach (var m in Model)
            {
                <div class="col-md-4">
                    <div class="circle">
                        <a href="#" class="circle-toggle text-pretty" data-toggle="popover" data-trigger="focus" data-content="<a href='/GroupMembers/Details/@m.Id' class='btn btn-success'>Track</button><a href='/GroupMembers/Edit/@m.Id' class='btn btn-pretty'>View</button>">@m.Name</a>
                    </div>
                </div>
            }

        </div>
    </div>
</div>

<div class="row text-center">
    <a href="@Url.Action("Index","GroupMembers")" class="btn btn-pretty pull-right">See List</a>

</div>


@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $('[data-toggle="popover"]').popover({
                container: 'body',
                html: true
            });
            $('.popover-dismiss').popover({
                trigger: 'focus'
            })
        });
    </script>
}